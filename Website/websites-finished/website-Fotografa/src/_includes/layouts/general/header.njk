<div class="header">

  <div class="container">

    <a href="/" class="header-logo">
      <img src="/assets/images/logo.svg" class="header-logo-normal" alt="MZ" >
      <img src="/assets/images/logo-inverso.svg" class="header-logo-inverso" alt="MZ" >
    </a>

    <button href="/" class="hamburger hamburger--collapse header-nav-icon" type="button">
      <span class="hamburger-box">
        <span class="hamburger-inner"></span>
      </span>
      {# <img src="/assets/images/nav.svg" alt="MZ" > #}
    </button>

    {% set navPages = collections.all | eleventyNavigation %}
    <div class="nav" id="navigation"> {# col-12 #}
      {%- for elemento in navPages %}
        <a href="{{ elemento.url | url }}" class="text-headline"{% endif %}>
          {{ elemento.title }}
        </a>
      {%- endfor %}
    </div>

  </div>

</div>